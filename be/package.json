{
  "name": "cad",
  "author": "Rheza Sindhuwinata <rheza.s@gmail.com>",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --minify --format esm",
    "dev": "bun run --watch src/index.ts",
    "start": "bun run dist/index.js",
    "lint": "bunx eslint . --ignore-pattern dist/",
    "format": "bunx prettier --write .",
    "lint:fix": "bunx eslint . --ignore-pattern dist/ --fix",
    "prepare": "husky"
  },
  "prisma": {
    "seed": "tsx ./prisma/seed.ts"
  },
  "lint-staged": {
    "**/*.{js,ts,json,md}": "prettier --write"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/bun": "latest",
    "@types/eslint-config-prettier": "^6.11.3",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript-eslint": "^8.23.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@elysiajs/swagger": "^1.2.0",
    "@prisma/client": "^6.3.0",
    "elysia": "^1.2.10",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "prisma": "^6.3.0"
  }
}
